		, ISNULL(T2.SubjectGrowthRate, 0)					AS SubjectGrowthRate
		, ISNULL(T2.SubjectTrendState, 1)					AS SubjectTrendState
		, ISNULL(T2.SubjectGrowthScore, 0)					AS SubjectGrowthScore
