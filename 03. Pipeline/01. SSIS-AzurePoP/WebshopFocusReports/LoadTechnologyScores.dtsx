    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{9DF95056-637F-48CF-AEBA-DEA3F4993E2F}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;&#xA;DECLARE @TopicScoresCatalog sysname = '&quot; + @[User::cfg_pkg_PatentScoresCatalog] + &quot;';&#xA;DECLARE @TopicScoresServer sysname = '&quot; + @[User::cfg_pkg_PatentScoresDataSource] + &quot;';&#xA;DECLARE @TrendEndYear smallint = &quot; + (DT_WSTR, 10)@[User::cfg_pkg_TrendEndYear] + &quot;;&#xA;&#xA;SELECT&#xA;&#x9;T5.[CompanyId]&#xA;&#x9;, T4.[SubjectId]&#xA;&#x9;, T7.[CalendarId]&#xA;&#x9;, -1 AS [DocumentId]&#xA;&#x9;, -1 AS [ParagraphId]&#xA;&#x9;, T6.[IndustryId]&#xA;&#x9;, -1 AS [SubjectScoreSourceId]&#xA;&#x9;, T9.[CountryId]&#xA;&#x9;, T1.[cosSimilarity]&#xA;&#x9;, MAX( CASE WHEN T7.[Year] BETWEEN @TrendEndYear - 1 AND @TrendEndYear THEN T1.[nr_of_topics_for_bvd_id] ELSE 0 END ) &#xA;&#x9;&#x9;&#x9;OVER( PARTITION BY T1.[bvd_id] &#xA;&#x9;&#x9;&#x9;&#x9;) AS [NrOfTopicsInTrendYear]&#xA;FROM &quot; + @[User::varTempTableFactSubjectStaging] + &quot; T1&#xA;INNER JOIN [result].[DimSubject] T4 ON T4.[TM_topic_server] = @TopicScoresServer AND T4.[TM_topic_catalog] = @TopicScoresCatalog AND T4.[TM_topic_id] = T1.[topic_id]&#xA;INNER JOIN [result].[DimCompany] T5 ON T5.[BvdId] = T1.[bvd_id]&#xA;INNER JOIN [result].[DimIndustry] T6 ON T6.[IndustryCodeLevel5] = T5.[NaceCode] AND T6.[IndustryCodeLevel1] = 'NACE_REV2'&#xA;INNER JOIN [result].[DimCalendar] T7 ON T7.[CalendarId] = CAST(CONCAT(T1.[priority_year], '0101') AS INT)&#xA;INNER JOIN [result].[DimCountry] T9 ON T9.[CountryCodeLevel4] = T5.[Country]&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="sqlSelectTopicScoresFromStaging">
      <DTS:VariableValue
        DTS:DataType="8">
DECLARE @TopicScoresCatalog sysname = 'pw_v2017a_001';
DECLARE @TopicScoresServer sysname = 'NLAGPDATACORE';
DECLARE @TrendEndYear smallint = 2016;

SELECT
	T5.[CompanyId]
	, T4.[SubjectId]
	, T7.[CalendarId]
	, -1 AS [DocumentId]
	, -1 AS [ParagraphId]
	, T6.[IndustryId]
	, -1 AS [SubjectScoreSourceId]
	, T9.[CountryId]
	, T1.[cosSimilarity]
	, MAX( CASE WHEN T7.[Year] BETWEEN @TrendEndYear - 1 AND @TrendEndYear THEN T1.[nr_of_topics_for_bvd_id] ELSE 0 END ) 
			OVER( PARTITION BY T1.[bvd_id] 
				) AS [NrOfTopicsInTrendYear]
FROM [dbo].[##factsubject_staging{72C0C558-C424-4536-AD51-816ECD41ABB6}] T1
INNER JOIN [result].[DimSubject] T4 ON T4.[TM_topic_server] = @TopicScoresServer AND T4.[TM_topic_catalog] = @TopicScoresCatalog AND T4.[TM_topic_id] = T1.[topic_id]
INNER JOIN [result].[DimCompany] T5 ON T5.[BvdId] = T1.[bvd_id]
INNER JOIN [result].[DimIndustry] T6 ON T6.[IndustryCodeLevel5] = T5.[NaceCode] AND T6.[IndustryCodeLevel1] = 'NACE_REV2'
INNER JOIN [result].[DimCalendar] T7 ON T7.[CalendarId] = CAST(CONCAT(T1.[priority_year], '0101') AS INT)
INNER JOIN [result].[DimCountry] T9 ON T9.[CountryCodeLevel4] = T5.[Country]
</DTS:VariableValue>
