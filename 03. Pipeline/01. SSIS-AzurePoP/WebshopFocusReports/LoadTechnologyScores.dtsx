INNER JOIN [result].[DimRisk] T4 ON T4.TM_topic_id = T1.topic_id
INNER JOIN [result].[DimCompany] T5 ON T5.BvdId = T2.current_bvd_id
INNER JOIN [result].DimIndustry T6 ON T6.NaceCodeLevel4 = T5.NaceCode</property>
