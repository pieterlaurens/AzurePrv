, AllBaseMeasures AS
(
	SELECT * FROM IndustryBaseMeasures T1
	UNION ALL
	SELECT * FROM BaseMeasuresAcrossIndustries T1
)

, CalculatedMeasures AS
(
	SELECT T1.[Year]
		, T1.[NaceCodeLevel1]
		, risk_id
		, risk_label
		, risk_level
		, nr_of_companies = T1.companies_per_industry_risk_year
		, nr_of_companies_in_industry = T1.companies_per_industry_year
		, nr_of_risks = T1.risks_per_industry_risk_year
		, nr_of_risks_in_industry = T1.risks_per_industry_year
		, relative_companycount_wrt_industry =
			CASE 
				WHEN T1.companies_per_industry_year = 0 THEN 0 
				ELSE T1.companies_per_industry_risk_year / T1.companies_per_industry_year
			END
		, relative_riskscore_wrt_industry =
			CASE
				WHEN T1.companies_per_industry_year = 0 THEN 0 
				ELSE T1.risks_per_industry_risk_year / T1.companies_per_industry_year
			END
		, relative_riskscore_wrt_risklevel =
			CASE
				WHEN T1.risks_per_industry_year = 0 THEN 0 
				ELSE T1.risks_per_industry_risk_year / T1.risks_per_industry_year
			END
	FROM AllBaseMeasures T1
)

, TrendMeasures AS 
(
	SELECT T2.[Year] 
			, T2.[NaceCodeLevel1]
			, T2.[risk_id]
			, T2.[risk_label]
			, T2.[risk_level]
			, RiskGrowthRate = 
					CASE 
						WHEN T1.nr_of_risks_in_industry = 0 THEN 0 
						ELSE (T2.relative_riskscore_wrt_industry -  T1.relative_riskscore_wrt_industry)
								/  T1.relative_riskscore_wrt_industry 
					END
			, RiskTrendState = 
					CASE 
						WHEN ( T2.relative_riskscore_wrt_industry - T1.relative_riskscore_wrt_industry ) &lt; @stableTrendThreshold THEN 1
						WHEN SIGN( T2.relative_riskscore_wrt_industry - T1.relative_riskscore_wrt_industry ) = -1 THEN 0
						ELSE 2
					END
			, RiskGrowthScore = 
					( T2.relative_riskscore_wrt_risklevel / 
						CASE 
							WHEN T1.relative_riskscore_wrt_risklevel = 0 THEN 1 
							ELSE T1.relative_riskscore_wrt_risklevel
						END 
					) 
					* SQRT( ABS( T2.relative_riskscore_wrt_risklevel - T1.relative_riskscore_wrt_risklevel ) )
					* SIGN( T2.relative_riskscore_wrt_risklevel - T1.relative_riskscore_wrt_risklevel )
			, IndustryGrowthRate = 
					CASE 
						WHEN T1.nr_of_companies_in_industry = 0 THEN 0 
						ELSE (T2.relative_companycount_wrt_industry -  T1.relative_companycount_wrt_industry)
								/ T1.relative_companycount_wrt_industry
					END
			, IndustryTrendState = 
					CASE 
						WHEN ( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry ) &lt; @stableTrendThreshold THEN 1
						WHEN SIGN( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry ) = -1 THEN 0
						ELSE 2
					END
			, IndustryGrowthScore = 
					( T2.relative_companycount_wrt_industry / 
						CASE 
							WHEN T1.relative_companycount_wrt_industry = 0 THEN 1 
							ELSE T1.relative_companycount_wrt_industry
						END 
					) 
					* SQRT( ABS( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry ) )
					* SIGN( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry )


	FROM CalculatedMeasures AS T1			-- Metrics In Current Year
	INNER JOIN CalculatedMeasures AS T2		-- Metrics In NextYear
		ON T2.[NaceCodeLevel1] = T1.[NaceCodeLevel1] AND T2.[risk_id] = T1.[risk_id]
		AND T2.[Year] = T1.[Year] + 1
)
SELECT T3.[IndustryId], T4.[RiskId], T5.[CalendarId], T1.*
		, ISNULL(T2.RiskGrowthRate, 0)					AS RiskGrowthRate
		, ISNULL(T2.RiskTrendState, 1)					AS RiskTrendState
		, ISNULL(T2.RiskGrowthScore, 0)					AS RiskGrowthScore
		, ISNULL(T2.IndustryGrowthRate, 0)				AS IndustryGrowthRate
		, ISNULL(T2.IndustryTrendState, 1)				AS IndustryTrendState
		, ISNULL(T2.IndustryGrowthScore, 0)				AS IndustryGrowthScore
FROM CalculatedMeasures T1
LEFT OUTER JOIN TrendMeasures T2 ON T2.[Year] = T1.[Year] AND T2.NaceCodeLevel1 = T1.NaceCodeLevel1 AND T2.risk_id = T1.risk_id AND T2.risk_level = T1.risk_level
INNER JOIN [result].[DimIndustry] T3 ON T3.[NaceCodeLevel4] = T1.[NaceCodeLevel1] AND T3.[Level] IN (0, 1)
INNER JOIN [result].[DimRisk] T4 ON T4.[RiskCodeLevel4] = T1.[risk_id] AND T4.[Level] = T1.[risk_level]
INNER JOIN [result].[DimCalendar] T5 ON T5.[Year] = T1.[Year] AND T5.[DayOfYear] = 1
ORDER BY T1.NaceCodeLevel1, T1.[Year], T1.risk_id, T1.risk_label, T1.risk_level</property>
