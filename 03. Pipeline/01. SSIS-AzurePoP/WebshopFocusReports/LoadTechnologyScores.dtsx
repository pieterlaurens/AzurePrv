DECLARE @DatasetNameList nvarchar(4000) = 'pyscrapp_dutch_risksections,sec_risk_paragraphs_39k'

; WITH topic_scores AS
(
	SELECT
		c.bvdId		AS document_id
		, a.topic_id
		, a.topic_terms_matched
		, a.cosSimilarity
	FROM [NLPdev].[dbo].[topic_scores] a 
	INNER JOIN [NLPdev].[dbo].[nlpArchive_textIDtoCompanyID] c ON a.documentId=c.documentId 
	INNER JOIN [NLPdev].[dbo].[topic] d ON a.topic_id=d.id
	WHERE a.cosSimilarity IS NOT NULL
	AND a.score_run_id = ( SELECT max(score_run_id) FROM [NLPdev].[dbo].[topic_scores] WHERE dsid = a.dsid AND topic_id = a.topic_id )
	AND a.dsid IN ( SELECT [id] FROM [NLPdev].[dbo].[nlpModule_datasetDesc] WHERE [name] IN ( select [string] from [str].[splitString] ( @DatasetNameList, ',') ) )
	AND d.project_id IN ( SELECT [id] FROM [NLPdev].[dbo].[topic_project] WHERE [name] IN ( select [string] from [str].[splitString] ( @ProjectNameList, ',') ) )
)

, doc_to_bvd_id AS
(
	SELECT DISTINCT [document_id], [current_bvd_id], [CIK] FROM [beursgorilla].[doc_to_bvdid]
	UNION ALL
	SELECT DISTINCT [document_id], [current_bvd_id], [CIK] FROM [sec].[doc_to_bvdid]
)

