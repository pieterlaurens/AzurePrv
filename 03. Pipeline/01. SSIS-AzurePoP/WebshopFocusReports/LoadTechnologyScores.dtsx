      DTS:ObjectName="sqlSelectCompanyMetrics">
      <DTS:VariableValue
        DTS:DataType="8">
DECLARE @trendEndYear int = 2016
			, @stableTrendThreshold float = 3;

WITH ScoresPerYear AS
(	SELECT [CompanyId]
			, [IndustryId]
			, [CountryId]
			, [BvdId]
			, [SubjectTreeCode]
			, [SubjectId]
			, [Year]
			, [LastYearReported]
			, [Score]
	FROM [dbo].[##scoresperyear{72C0C558-C424-4536-AD51-816ECD41ABB6}]
	-- Only include companies that do have (enough) occurrences in the trend end year
	WHERE [LastYearReported] BETWEEN @trendEndYear - 1 AND @trendEndYear
)

, CompanyBaseMeasures AS
(
	SELECT T1.[Year], T1.[CompanyId], T1.[BvdId], T1.[CountryId], T1.[IndustryId]
			, MAX(T1.[LastYearReported]) AS [LastYearReported]
			, T1.[SubjectTreeCode]											AS tree_code
			, 'RA'				AS subject_id
			, 0																						AS tree_level
			, CONVERT(float, 1)												AS companies_per_company_subject_year
			, CONVERT(float, 1)												AS companies_per_company_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.[SubjectTreeCode]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(SUM([Score]), 0))						AS occurrences_per_company_subject_year
			, CONVERT(float, ISNULL((
								SELECT SUM([Score])
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.[SubjectTreeCode]
								AND [CompanyId] = T1.[CompanyId]
							 ), 0)
						 )													AS occurrences_per_company_year
			, CONVERT(float, ISNULL((
								SELECT SUM([Score])
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
							 ), 0)
						 )													AS occurrences_per_year
	FROM ScoresPerYear T1
	INNER JOIN [result].[DimSubject] T2 ON T2.[SubjectId] = T1.[SubjectId]
	GROUP BY T1.[Year], T1.[CompanyId], T1.[CountryId], T1.[IndustryId], T1.[BvdId]
			, T1.[SubjectTreeCode]
)

, CalculatedMeasures AS
(
	SELECT T1.[Year]
		, T1.[BvdId], T1.[CountryId], T1.[IndustryId]
		, T1.[LastYearReported]
		, tree_code
		, subject_id
		, tree_level
		, nr_of_companies = T1.companies_per_company_subject_year
		, nr_of_companies_in_industry = T1.companies_per_company_year
		, nr_of_occurrences = T1.occurrences_per_company_subject_year
		, nr_of_occurrences_in_industry = T1.occurrences_per_company_year
		, relative_companycount_wrt_industry =
			CASE 
				WHEN T1.companies_per_company_year = 0 THEN 0 
				ELSE T1.companies_per_company_subject_year / T1.companies_per_company_year
			END
		, relative_score_wrt_industry =
			CASE
				WHEN T1.companies_per_company_year = 0 THEN 0 
				ELSE T1.occurrences_per_company_subject_year / T1.companies_per_company_year
			END
		, relative_score_wrt_treelevel =
			CASE
				WHEN T1.occurrences_per_company_year = 0 THEN 0 
				ELSE T1.occurrences_per_company_subject_year / T1.occurrences_per_company_year
			END
	FROM CompanyBaseMeasures T1
)

, TrendMeasures AS 
(
	SELECT T2.[Year] 
			, T2.[BvdId]
			, T2.[tree_code]
			, T2.[subject_id]
			, T2.[tree_level]
			, SubjectGrowthRate = 
					CASE 
						WHEN T1.nr_of_occurrences_in_industry = 0 THEN 0 
						ELSE (T2.relative_score_wrt_industry -  T1.relative_score_wrt_industry)
								/  T1.relative_score_wrt_industry 
					END
			, SubjectTrendState = 
					CASE 
						WHEN ( T2.relative_score_wrt_industry - T1.relative_score_wrt_industry ) &lt; @stableTrendThreshold THEN 1
						WHEN SIGN( T2.relative_score_wrt_industry - T1.relative_score_wrt_industry ) = -1 THEN 0
						ELSE 2
					END
			, SubjectGrowthScore = 
					( T2.relative_score_wrt_treelevel / 
						CASE 
							WHEN T1.relative_score_wrt_treelevel = 0 THEN 1 
							ELSE T1.relative_score_wrt_treelevel
						END 
					) 
					* SQRT( ABS( T2.relative_score_wrt_treelevel - T1.relative_score_wrt_treelevel ) )
					* SIGN( T2.relative_score_wrt_treelevel - T1.relative_score_wrt_treelevel )
			, CompanyGrowthRate = 
					CASE 
						WHEN T1.nr_of_companies_in_industry = 0 THEN 0 
						ELSE (T2.relative_companycount_wrt_industry -  T1.relative_companycount_wrt_industry)
								/ T1.relative_companycount_wrt_industry
					END
			, CompanyTrendState = 
					CASE 
						WHEN ( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry ) &lt; @stableTrendThreshold THEN 1
						WHEN SIGN( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry ) = -1 THEN 0
						ELSE 2
					END
			, CompanyGrowthScore = 
					( T2.relative_companycount_wrt_industry / 
						CASE 
							WHEN T1.relative_companycount_wrt_industry = 0 THEN 1 
							ELSE T1.relative_companycount_wrt_industry
						END 
					) 
					* SQRT( ABS( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry ) )
					* SIGN( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry )


	FROM CalculatedMeasures AS T1			-- Metrics In Current Year
	INNER JOIN CalculatedMeasures AS T2		-- Metrics In NextYear
		ON T2.[BvdId] = T1.[BvdId] AND T2.[tree_code] = T1.[tree_code] AND T2.[subject_id] = T1.[subject_id]
		AND T2.[Year] = T1.[Year] + 1
)

SELECT T1.*
		, ISNULL(T2.SubjectGrowthRate, 0)					AS SubjectGrowthRate
		, ISNULL(T2.SubjectTrendState, 1)					AS SubjectTrendState
		, ISNULL(T2.SubjectGrowthScore, 0)					AS SubjectGrowthScore
		, ISNULL(T2.CompanyGrowthRate, 0)					AS CompanyGrowthRate
		, ISNULL(T2.CompanyTrendState, 1)					AS CompanyTrendState
		, ISNULL(T2.CompanyGrowthScore, 0)					AS CompanyGrowthScore
		, T3.CompanyId, T4.SubjectId, T5.CalendarId
FROM CalculatedMeasures T1
LEFT OUTER JOIN TrendMeasures T2 ON T2.[Year] = T1.[Year] AND T2.[BvdId] = T1.[BvdId] AND T2.[tree_code] = T1.[tree_code] AND T2.subject_id = T1.subject_id AND T2.tree_level = T1.tree_level
INNER JOIN [result].[DimCompany] T3 ON T3.[BvdId] = T1.[BvdId]
INNER JOIN [result].[DimSubject] T4 ON T4.[SubjectCodeLevel4] = T1.[subject_id] AND T4.[Level] = T1.[tree_level] AND T4.[SubjectCodeLevel1] = T1.[tree_code]
INNER JOIN [result].[DimCalendar] T5 ON T5.[Year] = T1.[Year] AND T5.[DayOfYear] = 1
ORDER BY T1.[BvdId], T1.[Year], T1.subject_id, T1.LastYearReported
