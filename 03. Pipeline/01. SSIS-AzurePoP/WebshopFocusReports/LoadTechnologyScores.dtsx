                          version="1">
                          <components>
                            <component
                              refId="Package\Execute Feature\Populate fact tables\FactCompanySubjectMetrics\Foreach Loop Container 1\Load FactCompanyMetrics incremental\Get CompanyMetrics"
                              componentClassID="DTSAdapter.OLEDBSource.3"
                              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                              description="OLE DB Source"
                              name="Get CompanyMetrics"
                              usesDispositions="true"
                              version="7">
                              <properties>
                                <property
                                  dataType="System.Int32"
                                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                                  name="CommandTimeout">0</property>
                                <property
                                  dataType="System.String"
                                  description="Specifies the name of the database object used to open a rowset."
                                  name="OpenRowset"></property>
                                <property
                                  dataType="System.String"
                                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                                  name="OpenRowsetVariable"></property>
                                <property
                                  dataType="System.String"
                                  description="The SQL command to be executed."
                                  name="SqlCommand"
                                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @trendStartYear int = ?
		, @trendEndYear int = ?
		, @stableTrendThreshold float = ?
		, @subjectScoreSourceLabelList nvarchar(1024) = ?


; WITH ScoresPerYear AS
(	SELECT DISTINCT 
			T1.[CompanyId]
			, T4.[BvdId]
			, T5.[SubjectCodeLevel1]											AS SubjectTreeCode
			, T1.[SubjectId]
			, T2.[NaceCodeLevel1]
			, T3.[Year]
			, MAX(T3.[Year]) OVER( PARTITION BY T1.[CompanyId], T1.[SubjectId] ) AS LastYearReported
	FROM result.FactSubjectScore T1
	INNER JOIN result.DimIndustry T2 ON T2.IndustryId = T1.IndustryId
	INNER JOIN result.DimCalendar T3 ON T3.CalendarId = T1.CalendarId
	INNER JOIN result.DimCompany T4 ON T4.CompanyId = T1.CompanyId
	INNER JOIN result.DimSubject T5 ON T5.SubjectId = T1.SubjectId
	WHERE T3.[Year] BETWEEN @trendStartYear AND @trendEndYear
	-- Only include companies that have scores from the select score sources

	AND T1.[CompanyId] IN ( SELECT [CompanyId] 
								FROM [result].[FactSubjectScore] S1
								INNER JOIN [result].[DimSubjectScoreSource] S2 ON S2.[SubjectScoreSourceId] = S1.[SourceId]
								WHERE CHARINDEX(S2.[TM_source_label], @subjectScoreSourceLabelList) &gt; 0
							)
)

, CompanyBaseMeasures AS
(
	-- Risk Tree Level 1
	SELECT T1.[Year], T1.[CompanyId], T1.[BvdId], MAX(T1.[LastYearReported]) AS [LastYearReported]
			, T1.[SubjectTreeCode]											AS tree_code
			, T2.[SubjectCodeLevel1]										AS subject_id
			, T2.[SubjectLabelLevel1]										AS subject_label
			, 1																AS tree_level
			, CONVERT(float, 1)												AS companies_per_company_subject_year
			, CONVERT(float, 1)												AS companies_per_company_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.[SubjectTreeCode]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[SubjectId], '/', T1.[CompanyId]) 
										  ), 0))							AS occurrences_per_company_subject_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(SubjectId, '/', CompanyId))
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.[SubjectTreeCode]
								AND [CompanyId] = T1.[CompanyId]
							 ), 0)
						 )													AS occurrences_per_company_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(SubjectId, '/', CompanyId))
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
							 ), 0)
						 )													AS occurrences_per_year
	FROM ScoresPerYear T1
	INNER JOIN [result].[DimSubject] T2 ON T2.[SubjectId] = T1.[SubjectId]
	GROUP BY T1.[Year], T1.[CompanyId], T1.[BvdId], T1.[SubjectTreeCode], T2.[SubjectCodeLevel1], T2.[SubjectLabelLevel1]

	UNION ALL

	-- Risk Tree Level 2
	SELECT T1.[Year], T1.[CompanyId], T1.[BvdId], MAX(T1.[LastYearReported]) AS [LastYearReported]
			, T1.[SubjectTreeCode]											AS tree_code
			, T2.[SubjectCodeLevel2]										AS subject_id
			, T2.[SubjectLabelLevel2]										AS subject_label
			, 2																AS tree_level
			, CONVERT(float, 1)												AS companies_per_company_subject_year
			, CONVERT(float, 1)												AS companies_per_company_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[SubjectId], '/', T1.[CompanyId]) 
										  ), 0))							AS occurrences_per_company_subject_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(SubjectId, '/', CompanyId))
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
								AND [CompanyId] = T1.[CompanyId]
							 ), 0)
						 )													AS occurrences_per_company_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(SubjectId, '/', CompanyId))
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
							 ), 0)
						 )													AS occurrences_per_year
	FROM ScoresPerYear T1
	INNER JOIN [result].[DimSubject] T2 ON T2.[SubjectId] = T1.[SubjectId]
	GROUP BY T1.[Year], T1.[CompanyId], T1.[BvdId], T1.[SubjectTreeCode], T2.[SubjectCodeLevel2], T2.[SubjectLabelLevel2]

	UNION ALL

	-- Risk Tree Level 3
	SELECT T1.[Year], T1.[CompanyId], T1.[BvdId], MAX(T1.[LastYearReported]) AS [LastYearReported]
			, T1.[SubjectTreeCode]											AS tree_code
			, T2.[SubjectCodeLevel3]										AS subject_id
			, T2.[SubjectLabelLevel3]										AS subject_label
			, 3																AS tree_level
			, CONVERT(float, 1)												AS companies_per_company_subject_year
			, CONVERT(float, 1)												AS companies_per_company_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[SubjectId], '/', T1.[CompanyId]) 
										  ), 0))							AS occurrences_per_company_subject_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(SubjectId, '/', CompanyId))
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
								AND [CompanyId] = T1.[CompanyId]
							 ), 0)
						 )													AS occurrences_per_company_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT([SubjectId], '/', CompanyId))
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
							 ), 0)
						 )													AS occurrences_per_year
	FROM ScoresPerYear T1
	INNER JOIN [result].[DimSubject] T2 ON T2.[SubjectId] = T1.[SubjectId]
	GROUP BY T1.[Year], T1.[CompanyId], T1.[BvdId], T1.[SubjectTreeCode], T2.[SubjectCodeLevel3], T2.[SubjectLabelLevel3]

	UNION ALL

	-- Risk Tree Level 4
	SELECT T1.[Year], T1.[CompanyId], T1.[BvdId], MAX(T1.[LastYearReported]) AS [LastYearReported]
			, T1.[SubjectTreeCode]											AS tree_code
			, T2.[SubjectCodeLevel4]										AS subject_id
			, T2.[SubjectLabelLevel4]										AS subject_label
			, 4																AS tree_level
			, CONVERT(float, 1)												AS companies_per_company_subject_year
			, CONVERT(float, 1)												AS companies_per_company_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[SubjectId], '/', T1.[CompanyId]) 
										  ), 0))							AS occurrences_per_company_subject_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(SubjectId, '/', CompanyId))
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
								AND [CompanyId] = T1.[CompanyId]
							 ), 0)
						 )													AS occurrences_per_company_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(SubjectId, '/', CompanyId))
								FROM ScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [SubjectTreeCode] = T1.SubjectTreeCode
							 ), 0)
						 )													AS occurrences_per_year
	FROM ScoresPerYear T1
	INNER JOIN [result].[DimSubject] T2 ON T2.[SubjectId] = T1.[SubjectId]
	GROUP BY T1.[Year], T1.[CompanyId], T1.[BvdId], T1.[SubjectTreeCode], T2.[SubjectCodeLevel4], T2.[SubjectLabelLevel4]
)

, CalculatedMeasures AS
(
	SELECT T1.[Year]
		, T1.[BvdId]
		, T1.[LastYearReported]
		, tree_code
		, subject_id
		, subject_label
		, tree_level
		, nr_of_companies = T1.companies_per_company_subject_year
		, nr_of_companies_in_industry = T1.companies_per_company_year
		, nr_of_occurrences = T1.occurrences_per_company_subject_year
		, nr_of_occurrences_in_industry = T1.occurrences_per_company_year
		, relative_companycount_wrt_industry =
			CASE 
				WHEN T1.companies_per_company_year = 0 THEN 0 
				ELSE T1.companies_per_company_subject_year / T1.companies_per_company_year
			END
		, relative_score_wrt_industry =
			CASE
				WHEN T1.companies_per_company_year = 0 THEN 0 
				ELSE T1.occurrences_per_company_subject_year / T1.companies_per_company_year
			END
		, relative_score_wrt_treelevel =
			CASE
				WHEN T1.occurrences_per_company_year = 0 THEN 0 
				ELSE T1.occurrences_per_company_subject_year / T1.occurrences_per_company_year
			END
	FROM CompanyBaseMeasures T1
)

, TrendMeasures AS 
(
	SELECT T2.[Year] 
			, T2.[BvdId]
			, T2.[tree_code]
			, T2.[subject_id]
			, T2.[subject_label]
			, T2.[tree_level]
			, SubjectGrowthRate = 
					CASE 
						WHEN T1.nr_of_occurrences_in_industry = 0 THEN 0 
						ELSE (T2.relative_score_wrt_industry -  T1.relative_score_wrt_industry)
								/  T1.relative_score_wrt_industry 
					END
			, SubjectTrendState = 
					CASE 
						WHEN ( T2.relative_score_wrt_industry - T1.relative_score_wrt_industry ) &lt; @stableTrendThreshold THEN 1
						WHEN SIGN( T2.relative_score_wrt_industry - T1.relative_score_wrt_industry ) = -1 THEN 0
						ELSE 2
					END
			, SubjectGrowthScore = 
					( T2.relative_score_wrt_treelevel / 
						CASE 
							WHEN T1.relative_score_wrt_treelevel = 0 THEN 1 
							ELSE T1.relative_score_wrt_treelevel
						END 
					) 
					* SQRT( ABS( T2.relative_score_wrt_treelevel - T1.relative_score_wrt_treelevel ) )
					* SIGN( T2.relative_score_wrt_treelevel - T1.relative_score_wrt_treelevel )
			, CompanyGrowthRate = 
					CASE 
						WHEN T1.nr_of_companies_in_industry = 0 THEN 0 
						ELSE (T2.relative_companycount_wrt_industry -  T1.relative_companycount_wrt_industry)
								/ T1.relative_companycount_wrt_industry
					END
			, CompanyTrendState = 
					CASE 
						WHEN ( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry ) &lt; @stableTrendThreshold THEN 1
						WHEN SIGN( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry ) = -1 THEN 0
						ELSE 2
					END
			, CompanyGrowthScore = 
					( T2.relative_companycount_wrt_industry / 
						CASE 
							WHEN T1.relative_companycount_wrt_industry = 0 THEN 1 
							ELSE T1.relative_companycount_wrt_industry
						END 
					) 
					* SQRT( ABS( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry ) )
					* SIGN( T2.relative_companycount_wrt_industry - T1.relative_companycount_wrt_industry )


	FROM CalculatedMeasures AS T1			-- Metrics In Current Year
	INNER JOIN CalculatedMeasures AS T2		-- Metrics In NextYear
		ON T2.[BvdId] = T1.[BvdId] AND T2.[tree_code] = T1.[tree_code] AND T2.[subject_id] = T1.[subject_id]
		AND T2.[Year] = T1.[Year] + 1
)
