                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">with category_types ([Name])
	as
	(
	select N'company'
	union all select N'industry_nace_all'
	union all select N'industry_nace'
	)
	select T1.[Name] AS [Type], case 
			when T1.[Name] = 'IA' then N'industry_nace_all SourceID does not equal ''IA'''
			when count(distinct T2.[SourceID]) = 0 then N'Category type not present in [dbo].[CategorySet]' 
			else convert(nvarchar(10), count(1))
			end AS [Error]
			, CONVERT(nvarchar(1024), T1.[Name])		  AS [Value]
	from category_types T1
	left outer join [dbo].[CategorySet] T2 ON T2.[Type] = T1.[Name]
	where ( T1.[Name] = N'IA' AND T2.[SourceID] &lt;&gt; N'IA' )
	or ( T2.[SourceID] is null )
	group by T1.[Name]
	having count(distinct T2.[SourceID]) &lt;= 1</property>
