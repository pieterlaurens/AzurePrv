DECLARE @SubjectLevel tinyint = ?
DECLARE @SubjectLevelInReport tinyint = ?

SELECT T1.*
	, T1.NumberOfCompanies 	AS RelativeScoreToAggregationLevel
		, CONVERT(datetime, T4.[Date]) AS [Period]
		, T2.[SubjectCodeLevel4], T2.[SubjectLabelLevel4], @SubjectLevelInReport AS SubjectTreeLevel
		, T3.[BvdId]
		, 0		AS CompanyGrowthRate
		, 0		AS CompanyGrowthScore
