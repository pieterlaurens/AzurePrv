FROM [result].[FactSubjectCompanyMetrics] T1
INNER JOIN [result].[DimSubject] T2 ON T2.[SubjectId] = T1.[SubjectId]
INNER JOIN [result].[DimCompany] T3 ON T3.[CompanyId] = T1.[CompanyId]
INNER JOIN [result].[DimCalendar] T4 ON T4.[CalendarId] = T1.[CalendarId]
WHERE T2.[SubjectLabelLevel1] IN ( select [string] from [str].[splitString] (@SubjectTreesToPublish, '|') )
AND T2.[Level] = @SubjectLevel</property>
