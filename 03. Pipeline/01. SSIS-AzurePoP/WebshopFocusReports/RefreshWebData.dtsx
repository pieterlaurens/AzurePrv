                              UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @trendStartYear int = ?
		, @trendEndYear int = ?

SELECT DISTINCT T3.BvdId	AS bvd_id
			, T3.CompanyName							AS company_name
			, T3.City									AS city
			, T2.IndustryCodeLevel5						AS nace_code
			, T3.Category								AS category
			, T3.LastAvailableYearOrbisFinancials		AS last_available_year
			, T3.NumberOfEmployees						AS employees
			, T3.Revenue								AS revenue
			, T3.Ticker									AS ticker
			, T3.Exchange								AS exchange
			, T3.[Description]							AS [description]
			, T3.PrimaryBusiness						AS primaryBusiness
			, T2.IndustryCodeLevel2						AS nace_code_lvl1
			, T2.IndustryLabelLevel2					AS industry_nace_lvl1
			, T2.IndustryCodeLevel3						AS nace_code_lvl2
			, T2.IndustryLabelLevel3					AS industry_nace_lvl2
			, T4.CountryLabelLevel4						AS country
			, T4.Latitude								AS country_lat
			, T4.Longitude								AS country_long
			, T4.CountryLabelLevel3						AS region
			, T5.Latitude								AS region_lat
			, T5.Longitude								AS region_long
	FROM result.FactSubjectScore T1
	INNER JOIN result.DimIndustry T2 ON T2.IndustryId = T1.IndustryId
	INNER JOIN result.DimCompany T3 ON T3.CompanyId = T1.CompanyId
	INNER JOIN result.DimCountry T4 ON T4.CountryId = T1.CountryId AND T4.[Level] = 4
	INNER JOIN result.DimCountry T5 ON T5.CountryCodeLevel3 = T4.CountryCodeLevel3 AND T5.[Level] = 3
	WHERE EXISTS ( SELECT 1 
					FROM [result].[FactSubjectCompanyMetrics]
					WHERE CompanyId = T1.[CompanyId] 
					-- To exclude scores from companies that do no have (enough) risks reported in the trend end year
					AND LastYearReported BETWEEN @trendEndYear -1 AND @trendEndYear
					)

UNION

