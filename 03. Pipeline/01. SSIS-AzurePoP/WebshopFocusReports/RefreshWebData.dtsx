                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH number_of_companies AS
	(
		SELECT T1.[Type], YEAR(T2.[Period]) AS [yr], COUNT(DISTINCT T1.[SourceID]) AS [num_companies]
		FROM [dbo].[CategorySet] T1
		INNER JOIN [dbo].[TopicPerCategoryPerPeriodSet] T2 ON T2.[Category_Id] = T1.[Id]
		WHERE [Type] = N'company'
		GROUP BY [Type], YEAR(T2.[Period])
	)
	SELECT CASE
				WHEN T1.[num_companies] &lt;&gt; T2.[num_companies] THEN N'Inudstry frequencies do not match number of companies present for Period' 
			END						AS [Error]
			, N'IA'	AS [Type]
			, CONCAT(N'Period ', T1.[yr]
						, ' has industry frequency ', T1.[num_companies]
						, ' while statistics are present for ', T2.[num_companies],' companies')	AS [Value]
	FROM [dbo].[industry_sec_company_freq_per_year] T1
	LEFT OUTER JOIN number_of_companies T2 ON T2.[yr] = T1.[yr]
	WHERE nace_code_lvl1 = 'IA'
	AND T1.[num_companies] &lt;&gt; T2.[num_companies]</property>
