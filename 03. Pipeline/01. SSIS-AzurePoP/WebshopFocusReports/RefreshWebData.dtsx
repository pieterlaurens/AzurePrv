JOIN [result].[DimIndustry] T3 ON T3.[IndustryId] = T1.[IndustryId]
JOIN [result].[DimIndustry] T4 ON T4.[IndustryId] = T1.[IndustryIdCounterpart]
WHERE T1.[ValueOfDealsAsTarget] &gt; 0
AND T4.[IndustryCodeLevel3] &lt;&gt; 'N/A'
)
SELECT *
		, CONCAT('Deal'
					, RIGHT( CONCAT( N'0000', [dealrank])
							, 4)
