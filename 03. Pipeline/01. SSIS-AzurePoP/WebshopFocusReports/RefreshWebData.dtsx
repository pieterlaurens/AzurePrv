                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">-- Test aggregation levels present per category
	select CASE
			WHEN count(distinct T2.AggregationLevel) = 0 THEN N'No aggregation levels present in periodical stats for category' 
			WHEN count(distinct T2.AggregationLevel) &lt; 4 THEN N'Not all aggregation levels present in periodical stats for category' 
			WHEN min(T2.AggregationLevel) != 1 THEN N'Aggregation level 1 not found in periodical stats for category'
			WHEN max(T2.AggregationLevel) &lt; 4 THEN N'Aggregation level 4 not found in periodical stats for category' 
			WHEN count(distinct T3.AggregationLevel) = 0 THEN N'No aggregation levels present in total stats for category' 
			WHEN count(distinct T3.AggregationLevel) &lt; 4 THEN N'Not all aggregation levels present in total stats for category' 
			WHEN min(T3.AggregationLevel) != 1 THEN N'Aggregation level 1 not found in total stats for category'
			WHEN max(T3.AggregationLevel) &lt; 4 THEN N'Aggregation level 4 not found in total stats for category' 
			END	AS [Error]
			, T1.[Type]
			, T1.[Id] AS [Value]
	from [dbo].[CategorySet] T1
	left outer join [dbo].[TopicPerCategoryPerPeriodSet] T2 ON T2.Category_Id = T1.Id
	left outer join [dbo].[TopicPerCategoryTotalSet] T3 ON T3.Category_Id = T1.Id
	where T1.[Type] IN ( 'industry_nace', 'industry_nace_all' )
	group by T1.[Type], T1.[Id]
	having count(distinct T2.AggregationLevel) &lt; 4
	or min(T2.AggregationLevel) != 1
	or max(T2.AggregationLevel) &lt; 4
	or count(distinct T3.AggregationLevel) &lt; 4
	or min(T3.AggregationLevel) != 1
	or max(T3.AggregationLevel) &lt; 4
	order by T1.[Type], T1.[Id]</property>
