INNER JOIN [result].[DimCompany] T3 ON T3.[CompanyId] = T1.[CompanyId]
INNER JOIN [result].[DimCalendar] T4 ON T4.[CalendarId] = T1.[CalendarId]
WHERE T2.[Level] = @SubjectLevel</property>
