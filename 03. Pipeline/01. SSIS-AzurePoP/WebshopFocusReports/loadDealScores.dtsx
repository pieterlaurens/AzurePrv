                              name="SqlCommand"
                              UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @trendStartYear int = ?
	, @trendEndYear int = ?
 ;

SELECT DISTINCT 
			T1.[CompanyId]
			, T5.[BvdId]
			, T1.[CountryId]
			, T1.[IndustryId]
			, T3.[IndustryCodeLevel1]
			, T3.[IndustryCodeLevel2]
			, T3.[IndustryCodeLevel3]
			, T3.[IndustryCodeLevel4]
			, T3.[IndustryCodeLevel5]
			, T1.[IndustryIdCounterpart]
			, T2.[Year]
			, COUNT(DISTINCT CASE WHEN T1.[NumberOfDealsAsAcquirer] = 1 THEN T1.[DealID] END)	AS [NumberOfDealsAsAcquirer]
			, COUNT(DISTINCT CASE WHEN T1.[NumberOfDealsAsTarget] = 1 THEN T1.[DealID] END)		AS [NumberOfDealsAsTarget]
			, COUNT(DISTINCT CASE WHEN T1.[NumberOfDealsAsVendor] = 1 THEN T1.[DealID] END)		AS [NumberOfDealsAsVendor]
			, MAX(T2.[Year]) OVER( PARTITION BY T1.[CompanyId]) AS LastYearReported
	FROM [result].[FactDealScore] T1
	INNER JOIN [result].[DimCalendar] T2 ON T2.[CalendarId] = T1.[CalendarId]
	INNER JOIN [result].[DimIndustry] T3 ON T3.[IndustryId] = T1.[IndustryId]
	INNER JOIN [result].[DimCompany] T5 ON T5.[CompanyId] = T1.[CompanyId]
	WHERE T2.[Year] BETWEEN @trendStartYear AND @trendEndYear
	GROUP BY T1.[CompanyId]
				, T5.[BvdId]
				, T1.[CountryId]
				, T1.[IndustryId]
				, T3.[IndustryCodeLevel1]
				, T3.[IndustryCodeLevel2]
				, T3.[IndustryCodeLevel3]
				, T3.[IndustryCodeLevel4]
				, T3.[IndustryCodeLevel5]
				, T1.[IndustryIdCounterpart]
				, T2.[Year]
	;</property>
