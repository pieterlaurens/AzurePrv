                              name="SqlCommand"
                              UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @YearStart SMALLINT = ?
		, @YearEnd SMALLINT = ? ;
DECLARE @DateStart DATE = CONVERT(DATE, CONCAT(@YearStart, '0101'))
		, @DateEnd DATE = CONVERT(DATE, CONCAT(@YearEnd, '0101')) ;

SELECT YEAR(T1.announced_date) as [Year], T4.[seed_bvd_id] as bvd_id_acquirer, T5.[seed_bvd_id] as bvd_id_target, COUNT(DISTINCT T1.deal_id) as nr_of_deals, SUM(ISNULL(deal_value, 0)) as deal_value
FROM NLAGPDATACORE.sdd.[dbo].[deal_overview] T1
INNER JOIN NLAGPDATACORE.sdd.[dbo].[deal_acquirer] T2 ON T2.deal_id = T1.deal_id
INNER JOIN NLAGPDATACORE.sdd.[dbo].[deal_target] T3 ON T3.deal_id = T1.deal_id
INNER JOIN NLAGPDATACORE.company_ownership.[dbo].[ownership_full_lineage_per_seed_controlling_only] T4 ON T4.[subsidiary_bvd_id] = T2.[acquirer_bvd_id]
INNER JOIN NLAGPDATACORE.company_ownership.[dbo].[ownership_full_lineage_per_seed_controlling_only] T5 ON T4.[subsidiary_bvd_id] = T3.[target_bvd_id]
WHERE T1.deal_status_engine IN ('Completed','Completed Assumed')
AND T1.deal_type_engine IN ('Acquisition','Demerger','Merger','Minority stake','Joint venture')
AND T1.announced_date BETWEEN @DateStart AND @DateEnd
GROUP BY YEAR(T1.announced_date), T4.[seed_bvd_id], T5.[seed_bvd_id]</property>
