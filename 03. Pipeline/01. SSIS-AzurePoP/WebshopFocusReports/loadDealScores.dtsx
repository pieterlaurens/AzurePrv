      DTS:Namespace="User"
      DTS:ObjectName="sqlSelectDealScores">
      <DTS:VariableValue
        DTS:DataType="8">
WITH deal_scores AS
(
SELECT T1.[deal_id]																					AS deal_id
			, T1.[acquirer_bvd_id]																	AS bvd_id
			, T1.[target_bvd_id]																	AS bvd_id_counterpart
			, T1.[acquirer_role]																	AS [Role]
			, T1.[Year]																				AS [Year]
			, CEILING(ISNULL(SUM( 1.0 / (T1.[nr_of_targets]*T1.[nr_of_vendors])), 0))				AS NumberOfDealsAsAcquirer
			, CEILING(ISNULL(SUM(T1.[deal_value] / (T1.[nr_of_targets]*T1.[nr_of_vendors])), 0))	AS ValueOfDealsAsAcquirer
			, 0																						AS NumberOfDealsAsTarget
			, 0																						AS ValueOfDealsAsTarget
			, 0																						AS NumberOfDealsAsVendor
			, 0																						AS ValueOfDealsAsVendor
FROM [dbo].[##factdeal_staging] T1
WHERE T1.[acquirer_bvd_id] IS NOT NULL AND T1.[target_bvd_id] IS NOT NULL
GROUP BY T1.[deal_id], T1.[acquirer_bvd_id], T1.[target_bvd_id], T1.[acquirer_role], T1.[Year]

UNION ALL

SELECT T1.[deal_id]																					AS deal_id
			, T1.[target_bvd_id]																	AS bvd_id
			, T1.[acquirer_bvd_id]																	AS bvd_id_counterpart
			, NULL																					AS [Role]
			, T1.[Year]																				AS [Year]
			, 0																						AS NumberOfDealsAsAcquirer
			, 0																						AS ValueOfDealsAsAcquirer
			, CEILING(ISNULL(SUM(1.0 / (T1.[nr_of_acquirers]*T1.[nr_of_vendors])), 0))				AS NumberOfDealsAsTarget
			, CEILING(ISNULL(SUM(T1.[deal_value] / (T1.[nr_of_acquirers]*T1.[nr_of_vendors])), 0))	AS ValueOfDealsAsTarget
			, 0																						AS NumberOfDealsAsVendor
			, 0																						AS ValueOfDealsAsVendor
FROM [dbo].[##factdeal_staging] T1
WHERE T1.[target_bvd_id] IS NOT NULL AND T1.[acquirer_bvd_id] IS NOT NULL
GROUP BY T1.[deal_id], T1.[target_bvd_id], T1.[acquirer_bvd_id], T1.[Year]

UNION ALL

SELECT T1.[deal_id]																					AS deal_id
			, T1.[vendor_bvd_id]																	AS bvd_id
			, T1.[target_bvd_id]																	AS bvd_id_counterpart
			, T1.[vendor_role]																		AS [Role]
			, T1.[Year]																				AS [Year]
			, 0																						AS NumberOfDealsAsAcquirer
			, 0																						AS ValueOfDealsAsAcquirer
			, 0																						AS NumberOfDealsAsTarget
			, 0																						AS ValueOfDealsAsTarget
			, CEILING(ISNULL(SUM(1.0 / (T1.[nr_of_acquirers]*T1.[nr_of_targets])), 0))				AS NumberOfDealsAsVendor
			, CEILING(ISNULL(SUM(T1.[deal_value] / (T1.[nr_of_acquirers]*T1.[nr_of_targets])), 0))	AS ValueOfDealsAsVendor
FROM [dbo].[##factdeal_staging] T1
WHERE T1.[vendor_bvd_id] IS NOT NULL AND T1.[target_bvd_id] IS NOT NULL
GROUP BY T1.[deal_id], T1.[vendor_bvd_id], T1.[target_bvd_id], T1.[vendor_role], T1.[Year]

)

, deal_aggregate AS 
(
SELECT deal_id, bvd_id, bvd_id_counterpart, [Year]
			, ISNULL(MAX([Role]), N'Self')	AS [Role]
			, SUM(NumberOfDealsAsAcquirer)	AS NumberOfDealsAsAcquirer
			, SUM(ValueOfDealsAsAcquirer)	AS ValueOfDealsAsAcquirer
			, SUM(NumberOfDealsAsTarget)	AS NumberOfDealsAsTarget
			, SUM(ValueOfDealsAsTarget)		AS ValueOfDealsAsTarget
			, SUM(NumberOfDealsAsVendor)	AS NumberOfDealsAsVendor
			, SUM(ValueOfDealsAsVendor)		AS ValueOfDealsAsVendor
FROM deal_scores
GROUP BY deal_id, bvd_id, bvd_id_counterpart, [Year]
)

SELECT T2.CompanyId, T5.IndustryId, T7.CountryId
		, T4.CalendarId, T9.DealId
		, T3.CompanyId AS CompanyIdCounterPart
		, T6.IndustryId AS IndustryIdCounterPart
		, T8.CountryId AS CountryIdCounterPart
		, T1.[Role]
		, SUM(T1.NumberOfDealsAsAcquirer)	AS NumberOfDealsAsAcquirer
		, SUM(T1.ValueOfDealsAsAcquirer)	AS ValueOfDealsAsAcquirer
		, SUM(T1.NumberOfDealsAsTarget)		AS NumberOfDealsAsTarget
		, SUM(T1.ValueOfDealsAsTarget)		AS ValueOfDealsAsTarget
		, SUM(T1.NumberOfDealsAsVendor)		AS NumberOfDealsAsVendor
		, SUM(T1.ValueOfDealsAsVendor)		AS ValueOfDealsAsVendor
FROM deal_aggregate T1
INNER JOIN [result].[DimCompany] T2 ON T2.BvdId = T1.bvd_id
INNER JOIN [result].[DimCompany] T3 ON T3.BvdId = T1.bvd_id_counterpart
INNER JOIN [result].[DimCalendar] T4 ON T4.[Year] = T1.[Year] AND T4.[DayOfYear] = 1
INNER JOIN [result].[DimIndustry] T5 ON T5.IndustryCodeLevel5 = T2.NaceCode AND T5.IndustryCodeLevel1 = 'NACE_REV2'
INNER JOIN [result].[DimIndustry] T6 ON T6.IndustryCodeLevel5 = T3.NaceCode AND T6.IndustryCodeLevel1 = 'NACE_REV2'
INNER JOIN [result].[DimCountry] T7 ON T7.CountryCodeLevel4 = T2.Country
INNER JOIN [result].[DimCountry] T8 ON T8.CountryCodeLevel4 = T3.Country
INNER JOIN [result].[DimDeal] T9 ON T9.ZPH_deal_id = T1.deal_id
GROUP BY T2.CompanyId, T5.IndustryId, T7.CountryId
			, T4.CalendarId, T9.DealId, T1.[Role]
		, T3.CompanyId, T6.IndustryId, T8.CountryId
</DTS:VariableValue>
