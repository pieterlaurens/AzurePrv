                                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @trendStartYear int = ?
		, @trendEndYear int = ?
		, @stableTrendThreshold float = ?

; WITH RiskScoresPerYear AS
(	SELECT DISTINCT 
			T1.[CompanyId]
			, T4.[BvdId]
			, T1.[RiskId]
			, T2.[NaceCodeLevel1]
			, T3.[Year]
	FROM result.FactRiskScore T1
	INNER JOIN result.DimIndustry T2 ON T2.IndustryId = T1.IndustryId
	INNER JOIN result.DimCalendar T3 ON T3.CalendarId = T1.CalendarId
	INNER JOIN result.DimCompany T4 ON T4.CompanyId = T1.CompanyId
	WHERE T3.[Year] BETWEEN @trendStartYear AND @trendEndYear
	-- To exclude scores from companies that do no have (enough) risks reported in the trend end year
	AND T1.[CompanyId] IN ( SELECT CompanyId FROM result.FactCompanyMetrics WHERE LastYearReported BETWEEN @trendEndYear -1 AND @trendEndYear )
)

, IndustryBaseMeasures AS
(
	-- Risk Tree Level 1
	SELECT T1.[Year], T1.[NaceCodeLevel1]
			, T2.[RiskCodeLevel1]											AS risk_id
			, T2.[RiskLabelLevel1]											AS risk_label
			, 1																AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0))		AS companies_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [NaceCodeLevel1] = T1.[NaceCodeLevel1]
							), 0)
						 )													AS companies_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[RiskId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [NaceCodeLevel1] = T1.[NaceCodeLevel1]
							 ), 0)
						 )													AS risks_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS risks_per_year
	FROM RiskScoresPerYear T1
	INNER JOIN [result].[DimRisk] T2 ON T2.[RiskId] = T1.[RiskId]
	GROUP BY T1.[Year], T1.[NaceCodeLevel1], T2.[RiskCodeLevel1], T2.[RiskLabelLevel1]

	UNION ALL

	-- Risk Tree Level 2
	SELECT T1.[Year], T1.[NaceCodeLevel1]
			, T2.[RiskCodeLevel2]											AS risk_id
			, T2.[RiskLabelLevel2]											AS risk_label
			, 2																AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0))		AS companies_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [NaceCodeLevel1] = T1.[NaceCodeLevel1]
							 ), 0)
						 )													AS companies_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[RiskId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [NaceCodeLevel1] = T1.[NaceCodeLevel1]
							 ), 0)
						 )													AS risks_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS risks_per_year
	FROM RiskScoresPerYear T1
	INNER JOIN [result].[DimRisk] T2 ON T2.[RiskId] = T1.[RiskId]
	GROUP BY T1.[Year], T1.[NaceCodeLevel1], T2.[RiskCodeLevel2], T2.[RiskLabelLevel2]

	UNION ALL

	-- Risk Tree Level 3
	SELECT T1.[Year], T1.[NaceCodeLevel1]
			, T2.[RiskCodeLevel3]											AS risk_id
			, T2.[RiskLabelLevel3]											AS risk_label
			, 3																AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0) )	AS companies_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [NaceCodeLevel1] = T1.[NaceCodeLevel1]
							 ), 0)
						 )													AS companies_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[RiskId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [NaceCodeLevel1] = T1.[NaceCodeLevel1]
							 ), 0)
						 )													AS risks_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS risks_per_year
	FROM RiskScoresPerYear T1
	INNER JOIN [result].[DimRisk] T2 ON T2.[RiskId] = T1.[RiskId]
	GROUP BY T1.[Year], T1.[NaceCodeLevel1], T2.[RiskCodeLevel3], T2.[RiskLabelLevel3]

	UNION ALL

	-- Risk Tree Level 4
	SELECT T1.[Year], T1.[NaceCodeLevel1]
			, T2.[RiskCodeLevel4]											AS risk_id
			, T2.[RiskLabelLevel4]											AS risk_label
			, 4																AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0) )	AS companies_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [NaceCodeLevel1] = T1.[NaceCodeLevel1]
							 ), 0)
						 )													AS companies_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[RiskId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
								AND [NaceCodeLevel1] = T1.[NaceCodeLevel1]
							 ), 0)
						 )													AS risks_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS risks_per_year
	FROM RiskScoresPerYear T1
	INNER JOIN [result].[DimRisk] T2 ON T2.[RiskId] = T1.[RiskId]
	GROUP BY T1.[Year], T1.[NaceCodeLevel1], T2.[RiskCodeLevel4], T2.[RiskLabelLevel4]
)

