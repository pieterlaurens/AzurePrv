	UNION ALL

	-- Risk Tree Level 2 - All industries
	SELECT T1.[Year], 'IA'													AS NaceCodeLevel1
			, T2.[RiskCodeLevel2]											AS risk_id
			, T2.[RiskLabelLevel2]											AS risk_label
			, 2																AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0) )	AS companies_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS companies_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[RiskId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS risks_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS risks_per_year
	FROM RiskScoresPerYear T1
	INNER JOIN [result].[DimRisk] T2 ON T2.[RiskId] = T1.[RiskId]
	GROUP BY T1.[Year], T2.[RiskCodeLevel2], T2.[RiskLabelLevel2]

	UNION ALL

	-- Risk Tree Level 3 - All industries
	SELECT T1.[Year], 'IA'													AS NaceCodeLevel1
			, T2.[RiskCodeLevel3]											AS risk_id
			, T2.[RiskLabelLevel3]											AS risk_label
			, 3																AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0) )	AS companies_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS companies_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[RiskId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS risks_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS risks_per_year
	FROM RiskScoresPerYear T1
	INNER JOIN [result].[DimRisk] T2 ON T2.[RiskId] = T1.[RiskId]
	GROUP BY T1.[Year], T2.[RiskCodeLevel3], T2.[RiskLabelLevel3]

	UNION ALL

	-- Risk Tree Level 4 - All industries
	SELECT T1.[Year], 'IA'													AS NaceCodeLevel1
			, T2.[RiskCodeLevel4]											AS risk_id
			, T2.[RiskLabelLevel4]											AS risk_label
			, 4																AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0) )	AS companies_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS companies_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CompanyId)
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[RiskId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_industry_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
								FROM RiskScoresPerYear
								WHERE [Year] = T1.[Year]
							 ), 0)
						 )													AS risks_per_industry_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT(RiskId, '/', CompanyId))
