DECLARE @DateStart DATE = CONVERT(DATE, CONCAT(2007, '-01-01')) ;
DECLARE @DateEnd DATE = CONVERT(DATE, CONCAT(2016, '-01-01')) ;

WITH deal_actors AS
(
SELECT T1.[deal_id]
		, YEAR(T1.[deal_date_engine])									AS [Year]
		, ISNULL(T3.[ancestor_bvd_id], T2.[acquirer_bvd_id])			AS acquirer_bvd_id
		, CASE 
			WHEN T3.[ancestor_bvd_id] IS NULL THEN N'Self'
			ELSE N'Owner'
		  END															AS acquirer_role
		, ISNULL(T5.[ancestor_bvd_id], T4.[vendor_bvd_id])				AS vendor_bvd_id
		, CASE 
			WHEN T5.[ancestor_bvd_id] IS NULL THEN N'Self'
			ELSE N'Owner'
		  END															AS vendor_role
		, T6.[target_bvd_id]											AS target_bvd_id
		, ( SELECT COUNT(DISTINCT ISNULL(S2.[ancestor_bvd_id], ISNULL(S1.[acquirer_bvd_id], '')))
			FROM [dbo].[deal_acquirer] S1
			LEFT OUTER JOIN company_ownership.[dbo].[ownership_full_lineage_controlling_only] S2 ON S2.[subsidiary_bvd_id] = S1.[acquirer_bvd_id]
			WHERE S1.[deal_id] = T1.[deal_id]
			)															AS nr_of_acquirers
		, ( SELECT COUNT(DISTINCT [target_bvd_id])
			FROM [dbo].[deal_target]
			WHERE [deal_id] = T1.[deal_id]
			AND [target_bvd_id] IS NOT NULL
			)															AS nr_of_targets
		, ( SELECT COUNT(DISTINCT ISNULL(S2.[ancestor_bvd_id], ISNULL(S1.[vendor_bvd_id], '')))
			FROM [dbo].[deal_vendor] S1
			LEFT OUTER JOIN company_ownership.[dbo].[ownership_full_lineage_controlling_only] S2 ON S2.[subsidiary_bvd_id] = S1.[vendor_bvd_id]
			WHERE S1.[deal_id] = T1.[deal_id]
			)															AS nr_of_vendors
		, ( SELECT ISNULL([deal_value], 0)
			FROM [dbo].[deal_overview]
			WHERE [deal_id] = T1.[deal_id]
			)															AS deal_value
FROM [dbo].[deal_overview] T1
LEFT OUTER JOIN [dbo].[deal_acquirer] T2 ON T2.deal_id = T1.deal_id
LEFT OUTER JOIN company_ownership.[dbo].[ownership_full_lineage_controlling_only] T3 ON T3.[subsidiary_bvd_id] = T2.[acquirer_bvd_id]
LEFT OUTER JOIN [dbo].[deal_vendor] T4 ON T4.deal_id = T1.deal_id
LEFT OUTER JOIN company_ownership.[dbo].[ownership_full_lineage_controlling_only] T5 ON T5.[subsidiary_bvd_id] = T4.[vendor_bvd_id]
INNER JOIN [dbo].[deal_target] T6 ON T6.deal_id = T1.deal_id
-- WHERE ( T2.[acquirer_bvd_id] IS NOT NULL OR T4.[vendor_bvd_id] IS NOT NULL )
WHERE ( T3.[ancestor_bvd_id] IS NOT NULL OR T5.[ancestor_bvd_id] IS NOT NULL )
AND T6.[target_bvd_id] IS NOT NULL
AND T1.deal_status_engine IN ('Completed')
AND T1.deal_type_engine IN ('Acquisition','Demerger','Merger','Minority stake','Joint venture')
AND T1.[deal_date_engine] BETWEEN @DateStart AND @DateEnd
GROUP BY T1.[deal_id], YEAR(T1.[deal_date_engine])
		, T3.[ancestor_bvd_id], T5.[ancestor_bvd_id]
		, ISNULL(T3.[ancestor_bvd_id], T2.[acquirer_bvd_id])
		, ISNULL(T5.[ancestor_bvd_id], T4.[vendor_bvd_id])
		, T6.[target_bvd_id]
)

SELECT [deal_id], [Year], [deal_value]
		, [acquirer_bvd_id], [acquirer_role], [vendor_bvd_id], [vendor_role], [target_bvd_id]
		, case nr_of_acquirers when 0 then 1 else nr_of_acquirers end as nr_of_acquirers
		, case nr_of_targets when 0 then 1 else nr_of_targets end as nr_of_targets
		, case nr_of_vendors when 0 then 1 else nr_of_vendors end as nr_of_vendors
FROM deal_actors
