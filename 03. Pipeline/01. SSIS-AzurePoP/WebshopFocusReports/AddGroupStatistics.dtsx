                        <property
                          dataType="System.String"
                          description="The SQL command to be executed."
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @groupId INT = ?
DECLARE @startYear INT = ?
DECLARE @endYear INT = ?
DECLARE @stableTrendThreshold FLOAT = ?

		; WITH group_totals AS
		(
		SELECT T1.[TopicId]
			, T1.[Topic]
			, T1.[AggregationLevel]
			, T1.[Category_Id]
			, TotalScore		= SUM(T1.[TotalScore])
			, RelativeScoreCat	= CONVERT(FLOAT, SUM(T1.[TotalScore])) /  ( SUM(CONVERT(FLOAT, SUM(T1.[TotalScore]))) OVER (PARTITION BY T1.[AggregationLevel], T1.[Category_Id]) )
			, RelativeScore		= CONVERT(FLOAT, SUM(T1.[CompanyCount])) / SUM(T3.[group_size])
			, CompanyCount		= SUM(T1.[CompanyCount])
		FROM [dbo].[TopicPerCategoryPerPeriodSet] T1
		JOIN [dbo].[CategorySet] T2 on T2.[Id] = T1.[Category_Id]
		JOIN [dbo].[user_group_company_freq_per_year] T3 on T3.[group_id] = T2.SourceID and T3.yr = year(T1.[Period])
		WHERE T2.[Type] in ('user_defined_group')
		GROUP BY T1.[TopicId], T1.[Topic], T1.[AggregationLevel], T1.[Category_Id]
		)

		, group_stats_first_period_present AS
		(
		SELECT T1.*
				, T3.[group_size]
				, AverageScore					= T1.TotalScore / T3.[group_size]
				, AverageScoreAsDenominator		= case when T1.TotalScore = 0 then 1 else T1.TotalScore end / T3.[group_size]
		FROM [dbo].TopicPerCategoryPerPeriodSet T1
		JOIN [dbo].CategorySet T2 ON T2.Id = T1.Category_Id
		JOIN [dbo].[user_group_company_freq_per_year] T3 on T3.[group_id] = T2.SourceID and T3.yr = year(T1.[Period])
		where T2.[Type] in ('user_defined_group')
		and T1.[Period] = ( SELECT MIN([Period]) 
								FROM [dbo].TopicPerCategoryPerPeriodSet 
								WHERE Period &gt;= CONVERT(DATETIME, CONCAT(@startYear, '-01-01') )
								AND Category_Id = T1.Category_Id 
								AND TopicId = T1.TopicId )
		)

		, group_stats_last_period_present AS
		(
		SELECT T1.*
				, T3.[group_size]
				, AverageScore					= T1.TotalScore / T3.[group_size]
				, AverageScoreAsDenominator		= case when T1.TotalScore = 0 then 1 else T1.TotalScore end / T3.[group_size]
		FROM [dbo].TopicPerCategoryPerPeriodSet T1
		JOIN [dbo].CategorySet T2 ON T2.Id = T1.Category_Id
		JOIN [dbo].[user_group_company_freq_per_year] T3 on T3.[group_id] = T2.SourceID and T3.yr = year(T1.[Period])
		where T2.[Type] in ('user_defined_group')
		and T1.[Period] = ( SELECT MAX([Period]) 
								FROM [dbo].TopicPerCategoryPerPeriodSet 
								WHERE Period &lt;= CONVERT(DATETIME, CONCAT(@endYear, '-01-01') )
								AND Category_Id = T1.Category_Id
								AND TopicId = T1.TopicId )
		)

		SELECT
			t1.TopicId, t1.Topic, t1.AggregationLevel, t1.Category_Id
			, t1.TotalScore, t1.CompanyCount, t1.RelativeScoreCat, t1.RelativeScore
			, TrendState = 
			  ( case when abs(c.AverageScore - p.AverageScore) &lt; @stableTrendThreshold then 1 
					when sign(c.AverageScore - p.AverageScore) = -1 then 0 
					else 2 
				end ) 
				* ( case when sign(c.TrendState) &gt;= 0 then 1 else -1 end )
			, GrowthRate = ( c.AverageScore - p.AverageScore) / p.AverageScoreAsDenominator
			, GrowthScore = 
			  ( c.RelativeScoreToCategory / ( case when p.RelativeScoreToCategory = 0 then 1 else p.RelativeScoreToCategory end ) )
				* sqrt(abs(c.RelativeScoreToCategory - p.RelativeScoreToCategory))
				* sign(c.RelativeScoreToCategory - p.RelativeScoreToCategory)
			, CompanyCountTrendState = 
			  ( case when abs(c.RelativeScoreToAggregationLevel - p.RelativeScoreToAggregationLevel) &lt; @stableTrendThreshold then 1
					when sign(c.RelativeScoreToAggregationLevel - p.RelativeScoreToAggregationLevel) = -1 then 0 
					else 2 
				 end )
				* ( case when sign(c.CompanyCountTrendState) &gt;= 0 then 1 else -1 end ) 
			, CompanyCountGrowthRate = 
			  ( c.RelativeScoreToAggregationLevel - p.RelativeScoreToAggregationLevel) 
			   / ( case when p.RelativeScoreToAggregationLevel=0 then 1 else p.RelativeScoreToAggregationLevel end)
			, CompanyCountGrowthScore = 
			 ( c.RelativeScoreToAggregationLevel / ( case when p.RelativeScoreToAggregationLevel = 0 then 1 else p.RelativeScoreToAggregationLevel end ) )
			   * sqrt(abs(c.RelativeScoreToAggregationLevel - p.RelativeScoreToAggregationLevel))
			   * sign(c.RelativeScoreToAggregationLevel - p.RelativeScoreToAggregationLevel) 
		FROM group_totals t1 
		INNER JOIN group_stats_first_period_present as p 
				ON t1.TopicId=p.TopicId	AND t1.AggregationLevel=p.AggregationLevel AND t1.Category_Id=p.Category_Id
		INNER JOIN group_stats_last_period_present as c
				ON p.TopicId=c.TopicId AND p.AggregationLevel=c.AggregationLevel AND p.Category_Id=c.Category_Id
		WHERE t1.Category_Id = @groupId</property>
