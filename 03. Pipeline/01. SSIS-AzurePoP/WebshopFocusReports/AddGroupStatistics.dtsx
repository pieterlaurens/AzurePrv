                        <property
                          dataType="System.String"
                          description="The SQL command to be executed."
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @groupId int = ?
DECLARE @trendStartYear int = ?
		, @trendEndYear int = ?
		, @stableTrendThreshold float =  ?

; WITH RiskTree AS
(
  SELECT T1.[Id] AS [RiskCodeLevel1], T1.[Name] AS [RiskLabelLevel1], T1.[Level] AS [RiskLevel1]
		, T2.[Id] AS [RiskCodeLevel2], T2.[Name] AS [RiskLabelLevel2], T2.[Level] AS [RiskLevel2]
		, T3.[Id] AS [RiskCodeLevel3], T3.[Name] AS [RiskLabelLevel3], T3.[Level] AS [RiskLevel3]
		, T4.[Id] AS [RiskCodeLevel4], T4.[Name] AS [RiskLabelLevel4], T4.[Level] AS [RiskLevel4]
  FROM [dbo].[TopicTreeSet] T1
  INNER JOIN [dbo].[TopicTreeSet] T2 ON T2.[ParentId] = T1.[Id]
  INNER JOIN [dbo].[TopicTreeSet] T3 ON T3.[ParentId] = T2.[Id]
  INNER JOIN [dbo].[TopicTreeSet] T4 ON T4.[ParentId] = T3.[Id]
  WHERE T1.[Level] = 1
 )

, RiskScoresPerYear AS
(	SELECT DISTINCT 
			T1.[Category_Id]		AS [CompanyId]
			, T1.[TopicId]			AS [TopicId]
			, T3.[Id]				AS [GroupId]
			, T1.[Period]
	FROM [dbo].[TopicPerCategoryPerPeriodSet] T1
	INNER JOIN [dbo].[CategorySet] T2 ON T2.[Id] = T1.[Category_Id]
	INNER JOIN	(
					SELECT T1.[Id], T1.[Type], T2.[BvdId]
					FROM [dbo].[CategorySet] T1 
					CROSS APPLY (SELECT [string] AS BvdId FROM [dbo].[splitString](T1.[Description], ',') WHERE [Id] = T1.[Id]) T2
					WHERE T1.[Type] = 'user_defined_group'
				) T3 ON T3.[BvdId] = T2.[SourceID]
	WHERE T1.[AggregationLevel] = 4
	AND T2.[Type] = 'company'
	AND T1.[Period] BETWEEN CONVERT(DATETIME, CONCAT(@trendStartYear, '-01-01')) AND CONVERT(DATETIME, CONCAT(@trendEndYear, '-01-01'))
)

, GroupBaseMeasures AS
(
	-- Risk Tree Level 1
	SELECT T1.[Period], T1.[GroupId]
			, T2.[RiskCodeLevel1]								AS risk_id
			, T2.[RiskLabelLevel1]								AS risk_label
			, T2.[RiskLevel1]									AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0))		AS companies_per_group_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT [CompanyId])
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
								AND [GroupId] = T1.[GroupId]
							), 0)
						 )													AS companies_per_group_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT [CompanyId])
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[TopicId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_group_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT([TopicId], '/', [CompanyId]))
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
								AND [GroupId] = T1.[GroupId]
							 ), 0)
						 )													AS risks_per_group_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT([TopicId], '/', [CompanyId]))
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
							 ), 0)
						 )													AS risks_per_year
	FROM RiskScoresPerYear T1
	INNER JOIN RiskTree T2 ON T2.[RiskCodeLevel4] = T1.[TopicId]
	GROUP BY T1.[Period], T1.[GroupId], T2.[RiskCodeLevel1], T2.[RiskLabelLevel1], T2.[RiskLevel1]

	UNION ALL

	-- Risk Tree Level 2
	SELECT T1.[Period], T1.[GroupId]
			, T2.[RiskCodeLevel2]											AS risk_id
			, T2.[RiskLabelLevel2]											AS risk_label
			, 2																AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0))		AS companies_per_group_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT [CompanyId])v
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
								AND [GroupId] = T1.[GroupId]
							 ), 0)
						 )													AS companies_per_group_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT [CompanyId])
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[TopicId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_group_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT([TopicId], '/', [CompanyId]))
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
								AND [GroupId] = T1.[GroupId]
							 ), 0)
						 )													AS risks_per_group_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT([TopicId], '/', [CompanyId]))
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
							 ), 0)
						 )													AS risks_per_year
	FROM RiskScoresPerYear T1
	INNER JOIN RiskTree T2 ON T2.[RiskCodeLevel4] = T1.[TopicId]
	GROUP BY T1.[Period], T1.[GroupId], T2.[RiskCodeLevel2], T2.[RiskLabelLevel2], T2.[RiskLevel1]

	UNION ALL

	-- Risk Tree Level 3
	SELECT T1.[Period], T1.[GroupId]
			, T2.[RiskCodeLevel3]											AS risk_id
			, T2.[RiskLabelLevel3]											AS risk_label
			, 3																AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0) )	AS companies_per_group_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT [CompanyId])
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
								AND [GroupId] = T1.[GroupId]
							 ), 0)
						 )													AS companies_per_group_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT [CompanyId])
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[TopicId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_group_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT([TopicId], '/', [CompanyId]))
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
								AND [GroupId] = T1.[GroupId]
							 ), 0)
						 )													AS risks_per_group_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT([TopicId], '/', [CompanyId]))
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
							 ), 0)
						 )													AS risks_per_year
	FROM RiskScoresPerYear T1
	INNER JOIN RiskTree T2 ON T2.[RiskCodeLevel4] = T1.[TopicId]
	GROUP BY T1.[Period], T1.[GroupId], T2.[RiskCodeLevel3], T2.[RiskLabelLevel3]

	UNION ALL

	-- Risk Tree Level 4
	SELECT T1.[Period], T1.[GroupId]
			, T2.[RiskCodeLevel4]											AS risk_id
			, T2.[RiskLabelLevel4]											AS risk_label
			, 4																AS risk_level
			, CONVERT(float, ISNULL(COUNT(DISTINCT T1.[CompanyId]), 0) )	AS companies_per_group_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT [CompanyId])
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
								AND [GroupId] = T1.[GroupId]
							 ), 0)
						 )													AS companies_per_group_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT [CompanyId])
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
							 ), 0)
						 )													AS companies_per_year
			, CONVERT(float, ISNULL(COUNT(DISTINCT 
											CONCAT(T1.[TopicId], '/', T1.[CompanyId]) 
										  ), 0))							AS risks_per_group_risk_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT([TopicId], '/', [CompanyId]))
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
								AND [GroupId] = T1.[GroupId]
							 ), 0)
						 )													AS risks_per_group_year
			, CONVERT(float, ISNULL((
								SELECT COUNT(DISTINCT CONCAT([TopicId], '/', [CompanyId]))
								FROM RiskScoresPerYear
								WHERE [Period] = T1.[Period]
							 ), 0)
						 )													AS risks_per_year
	FROM RiskScoresPerYear T1
	INNER JOIN RiskTree T2 ON T2.[RiskCodeLevel4] = T1.[TopicId]
	GROUP BY T1.[Period], T1.[GroupId], T2.[RiskCodeLevel4], T2.[RiskLabelLevel4]
)

, CalculatedMeasures AS
(
	SELECT T1.[Period]
		, T1.[GroupId]
		, risk_id, risk_label, risk_level
		, nr_of_companies = T1.companies_per_group_risk_year
		, nr_of_companies_in_group = T1.companies_per_group_year
		, nr_of_risks = T1.risks_per_group_risk_year
		, nr_of_risks_in_group = T1.risks_per_group_year
		, relative_companycount_wrt_group =
			CASE 
				WHEN T1.companies_per_group_year = 0 THEN 0 
				ELSE T1.companies_per_group_risk_year / T1.companies_per_group_year
			END
		, relative_riskscore_wrt_group =
			CASE
				WHEN T1.companies_per_group_year = 0 THEN 0 
				ELSE T1.risks_per_group_risk_year / T1.companies_per_group_year
			END
		, relative_riskscore_wrt_risklevel =
			CASE
				WHEN T1.risks_per_group_year = 0 THEN 0 
				ELSE T1.risks_per_group_risk_year / T1.risks_per_group_year
			END
	FROM GroupBaseMeasures T1
)

, TrendMeasures AS 
(
	SELECT T2.[Period] 
			, T2.[GroupId]
			, T2.[risk_id], T2.[risk_label], T2.[risk_level]
			, RiskGrowthRate = 
					CASE 
						WHEN T1.nr_of_risks_in_group = 0 THEN 0 
						ELSE (T2.relative_riskscore_wrt_group -  T1.relative_riskscore_wrt_group)
								/  T1.relative_riskscore_wrt_group
					END
			, RiskTrendState = 
					CASE 
						WHEN ( T2.relative_riskscore_wrt_group - T1.relative_riskscore_wrt_group ) &lt; @stableTrendThreshold THEN 1
						WHEN SIGN( T2.relative_riskscore_wrt_group - T1.relative_riskscore_wrt_group) = -1 THEN 0
						ELSE 2
					END
			, RiskGrowthScore = 
					( T2.relative_riskscore_wrt_risklevel / 
						CASE 
							WHEN T1.relative_riskscore_wrt_risklevel = 0 THEN 1 
							ELSE T1.relative_riskscore_wrt_risklevel
						END 
					) 
					* SQRT( ABS( T2.relative_riskscore_wrt_risklevel - T1.relative_riskscore_wrt_risklevel ) )
					* SIGN( T2.relative_riskscore_wrt_risklevel - T1.relative_riskscore_wrt_risklevel )
			, GroupGrowthRate = 
					CASE 
						WHEN T1.nr_of_companies_in_group = 0 THEN 0 
						ELSE (T2.relative_companycount_wrt_group-  T1.relative_companycount_wrt_group)
								/ T1.relative_companycount_wrt_group
					END
			, GroupTrendState = 
					CASE 
						WHEN ( T2.relative_companycount_wrt_group- T1.relative_companycount_wrt_group) &lt; @stableTrendThreshold THEN 1
						WHEN SIGN( T2.relative_companycount_wrt_group - T1.relative_companycount_wrt_group) = -1 THEN 0
						ELSE 2
					END
			, GroupGrowthScore = 
					( T2.relative_companycount_wrt_group / 
						CASE 
							WHEN T1.relative_companycount_wrt_group= 0 THEN 1 
							ELSE T1.relative_companycount_wrt_group
						END 
					) 
					* SQRT( ABS( T2.relative_companycount_wrt_group- T1.relative_companycount_wrt_group) )
					* SIGN( T2.relative_companycount_wrt_group- T1.relative_companycount_wrt_group)


	FROM CalculatedMeasures AS T1			-- Metrics In Current Year
	INNER JOIN CalculatedMeasures AS T2		-- Metrics In NextYear
		ON T2.[GroupId] = T1.[GroupId] AND T2.[risk_id] = T1.[risk_id]
		AND T2.[Period] = DATEADD(YY, 1, T1.[Period])
)

SELECT T1.*
		, ISNULL(T2.RiskGrowthRate, 0)					AS RiskGrowthRate
		, ISNULL(T2.RiskTrendState, 1)					AS RiskTrendState
		, ISNULL(T2.RiskGrowthScore, 0)					AS RiskGrowthScore
		, ISNULL(T2.GroupGrowthRate, 0)					AS GroupGrowthRate
		, ISNULL(T2.GroupTrendState, 1)					AS GroupTrendState
		, ISNULL(T2.GroupGrowthScore, 0)				AS GroupGrowthScore
FROM CalculatedMeasures T1
LEFT OUTER JOIN TrendMeasures T2 ON T2.[Period] = T1.[Period] AND T2.[GroupId] = T1.[GroupId] AND T2.risk_id = T1.risk_id
WHERE T1.[GroupId] = @GroupId
ORDER BY T1.[GroupId], T1.[Period], T1.risk_id</property>
