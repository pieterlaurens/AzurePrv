                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @EntityType NVARCHAR(1024) = ? ;

WITH  orbis_trade_descriptions 
AS
( SELECT [bvd_id], convert(nvarchar(4000), [text_content]) AS text_content FROM [dbo].[orbis_text] WHERE [text_type] = 'TRADDESCEN' )

, orbis_business_line
AS
( SELECT [bvd_id], convert(nvarchar(1024), [text_content]) AS text_content FROM [dbo].[orbis_text] WHERE [text_type] = 's_innod_primary_business_line' )

SELECT T1.bvd_id
		, T1.company_name
		, ISNULL(T1.country, 'N/A') AS country
		, T1.city
		, ISNULL(T1.nace_code, 'N/A') AS nace_code
		, T1.category
		, T2.[last_available_year]
		, T2.[employees]
		, T2.[revenue]
		, ISNULL(T3.ticker, '')								AS ticker
		, ISNULL(T3.main_exchange, '')						AS exchange
		, ISNULL(T4.text_content, '')						AS [description]
		, ISNULL(T5.text_content, '')						AS [primaryBusiness]
FROM [dbo].[orbis_basic] T1
LEFT OUTER JOIN [dbo].[financials_lay] T2
	ON T2.[bvd_id] = T1.[bvd_id]
LEFT OUTER JOIN [dbo].[orbis_stock] T3
	ON T3.[bvd_id] = T1.[bvd_id]
LEFT OUTER JOIN orbis_trade_descriptions T4
	ON T4.[bvd_id] = T1.[bvd_id]
LEFT OUTER JOIN orbis_business_line T5
	ON T5.bvd_id = T1.[bvd_id]
/*
WHERE ( CHARINDEX([entity_type], @EntityType) &gt; 0 
		OR [entity_type] IS NULL
		OR CHARINDEX('All', @EntityType) &gt; 0
	  )
*/
ORDER BY [bvd_id]</property>
